{
  "version": 2,
  "builds": [
    {
      "src": "frontend/src/index.tsx",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/music/(.*)",
      "dest": "/frontend/static/music/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/src/index.tsx"
    }
  ],
  "functions": {
    "frontend/src/index.tsx": {
      "includeFiles": "frontend/static/**"
    }
  }
}
